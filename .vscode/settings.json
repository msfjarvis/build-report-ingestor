{
  "deno.enable": true,
  "deno.unstable": true,
  "deno.lint": true,
  "nix.serverSettings": {
    "nil": {
      "diagnostics": {
        "ignored": ["unused_binding", "unused_with"]
      },
      "formatting": {
        "command": ["nixfmt"]
      }
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
}
